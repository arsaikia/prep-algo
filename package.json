{
  "name": "prep-algo",
  "version": "1.0.0",
  "description": "A platform for practicing algorithm problems with code execution and test cases",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "cd backend && nodemon --experimental-json-modules server.js",
    "client": "cd frontend && npm start --max_old_space_size=4096",
    "start": "cd backend && node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-server": "cd backend && npm install",
    "start-server": "cd backend && node server.js",
    "install-client": "cd frontend && npm install",
    "build-client": "cd frontend && npm run build",
    "start-client": "cd frontend && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arsaikia/prep-algo.git"
  },
  "keywords": [
    "prep-algo",
    "algorithms",
    "coding-practice"
  ],
  "author": "Arunabh Saikia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arsaikia/prep-algo/issues"
  },
  "homepage": "https://github.com/arsaikia/prep-algo#readme",
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "compression": "^1.8.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "google-auth-library": "^9.15.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "node-cache": "^5.1.2",
    "octokit": "^2.1.0",
    "react-cookie": "^4.1.1",
    "uuid": "^8.3.2",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "concurrency": "^0.1.4",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "nodemon": "^2.0.6"
  }
}